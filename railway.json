{
  "builds": [
    {
      "src": "index.js",
      "use": "@railway/rails-node"
    }
  ],
  "env": {
    "DISCORD_TOKEN": "your-discord-bot-token",  // This will be fetched from Railway secrets
    "PREFIX": "!",  // This will be fetched from Railway secrets
    "SPOTIFY_CLIENT_ID": "your-spotify-client-id",  // This will be fetched from Railway secrets
    "SPOTIFY_CLIENT_SECRET": "your-spotify-client-secret"  // This will be fetched from Railway secrets
  }
}
